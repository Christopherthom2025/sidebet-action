# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_PROJECT_REF=your-project-ref
SUPABASE_ACCESS_TOKEN=your-access-token

# Frontend Environment (React)
REACT_APP_SUPABASE_URL=${SUPABASE_URL}
REACT_APP_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Mobile Environment (React Native)
REACT_NATIVE_SUPABASE_URL=${SUPABASE_URL}
REACT_NATIVE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
SENTRY_DSN=your-sentry-dsn

# Deployment Tokens
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-org-id
VERCEL_PROJECT_ID=your-project-id

# iOS Deployment
APPLE_ID=your-apple-id
APPLE_PASSWORD=your-app-specific-password
MATCH_PASSWORD=your-match-password
FASTLANE_USER=${APPLE_ID}
FASTLANE_PASSWORD=${APPLE_PASSWORD}

# Android Deployment
PLAYSTORE_JSON_KEY=your-service-account-json-base64

# GitHub Actions
GH_TOKEN=your-github-token